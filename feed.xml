---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Daily AI Trend Reporter</title>
    <description>Automatically updated daily AI research digest</description>
    <link>https://yupengtang.github.io/Daily-AI-Trend-Reporter/</link>
    <atom:link href="https://yupengtang.github.io/Daily-AI-Trend-Reporter/feed.xml" rel="self" type="application/rss+xml" />
    <language>en</language>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    {% for post in site.posts limit:10 %}
    <item>
      <guid isPermaLink="true">https://yupengtang.github.io/Daily-AI-Trend-Reporter{{ post.url }}</guid>
      <title>{{ post.title | xml_escape }}</title>
      <link>https://yupengtang.github.io/Daily-AI-Trend-Reporter{{ post.url }}</link>
      <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
      <description><![CDATA[{{ post.content | strip_html | truncatewords: 200 }}]]></description>
    </item>
    {% endfor %}
  </channel>
</rss> 